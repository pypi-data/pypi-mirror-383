# Include documentation and configuration files
include README.md
include LICENSE
include pyproject.toml


# Include all protobuf files and Python packages
recursive-include akash *.py
recursive-include akash *.proto
recursive-include akash *.pyi

# Include type stubs
include akash/py.typed

# Exclude unnecessary files
exclude .gitignore
exclude .env*
exclude *.log
exclude temp_proto_deps

# Exclude development and build files
prune .git
prune __pycache__
prune *.egg-info
prune build
prune dist
prune .pytest_cache
prune .mypy_cache

# Exclude IDE files
prune .vscode
prune .idea
prune *.swp
prune *.swo