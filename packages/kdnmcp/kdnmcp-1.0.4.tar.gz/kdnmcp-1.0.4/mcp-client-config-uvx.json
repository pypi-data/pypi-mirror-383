{
  "mcpServers": {
    "kdniao-mcp": {
      "command": "python",
      "args": ["-m", "kdnmcp", "--transport", "stdio"],
      "env": {
        "EBUSINESS_ID": "<YOUR_EBUSINESS_ID>",
        "API_KEY": "<YOUR_API_KEY>",
        "LOG_LEVEL": "INFO",
        "PYTHONIOENCODING": "utf-8"
      }
    }
  }
}