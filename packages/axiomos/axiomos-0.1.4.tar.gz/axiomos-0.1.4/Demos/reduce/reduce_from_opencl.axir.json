{
  "version": "0.2",
  "meta": {
    "source_lang": "OpenCL"
  },
  "ops": [
    {
      "op": "DeviceSelect",
      "device": "auto"
    },
    {
      "op": "Comment",
      "text": "kernel k -> reduce_sum"
    },
    {
      "op": "DeviceMalloc",
      "dst": "&dA",
      "bytes": "N*sizeof(float)"
    },
    {
      "op": "DeviceMalloc",
      "dst": "&dOut",
      "bytes": "sizeof(float)"
    },
    {
      "op": "Memcpy",
      "dst": "dA",
      "src": "hA",
      "bytes": "N*sizeof(float)",
      "kind": "H2D"
    },
    {
      "op": "Comment",
      "text": "setarg k[0]=dA"
    },
    {
      "op": "Comment",
      "text": "setarg k[1]=dOut"
    },
    {
      "op": "Comment",
      "text": "setarg k[2]=N"
    },
    {
      "op": "KernelLaunch",
      "kernel": "reduce_sum",
      "grid": [
        "&global",
        "1",
        "1"
      ],
      "block": [
        "&local",
        "1",
        "1"
      ],
      "args": [
        "dA",
        "dOut",
        "N"
      ]
    },
    {
      "op": "Memcpy",
      "dst": "hOut",
      "src": "dOut",
      "bytes": "sizeof(float)",
      "kind": "D2H"
    },
    {
      "op": "DeviceSynchronize"
    },
    {
      "op": "DeviceFree",
      "ptr": "dA"
    },
    {
      "op": "DeviceFree",
      "ptr": "dOut"
    }
  ]
}