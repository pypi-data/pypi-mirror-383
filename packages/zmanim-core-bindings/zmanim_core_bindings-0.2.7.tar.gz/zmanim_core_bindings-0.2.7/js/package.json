{
  "name": "zmanim-core-bindings",
  "version": "0.2.6",
  "description": "Bindings to the zmanim-core rust library",
  "keywords": [
    "zmanim",
    "jewish",
    "calendar",
    "astronomy",
    "time",
    "zmanim-core"
  ],
  "author": "Moshe Dicker <dickermoshe@gmail.com> (https://github.com/dickermoshe)",
  "license": "GNU LGPLv2.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dickermoshe/zmanim-core.git"
  },
  "homepage": "https://github.com/dickermoshe/zmanim-core/blob/main/js/README.md",
  "bugs": {
    "url": "https://github.com/dickermoshe/zmanim-core/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:rust": "ubrn build web",
    "build": "vite build && tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.3",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "dependencies": {
    "uniffi-bindgen-react-native": "^0.29.3-1"
  }
}
