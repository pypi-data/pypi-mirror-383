# MultiAgent Project Environment Configuration
# Copy this to .env and customize with your API keys

# AI Integration APIs
GEMINI_API_KEY=your-gemini-api-key-here
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
OPENAI_API_KEY=your-openai-api-key-here

# GitHub Integration
GITHUB_TOKEN=your-github-token-here
GITHUB_USERNAME=your-github-username

# Debug and Logging
DEBUG=false
LOG_LEVEL=info

# Project Settings
PROJECT_NAME=${PROJECT_NAME:-multiagent-project}
ENVIRONMENT=${ENVIRONMENT:-development}

# Security
ALLOW_SUDO_OPERATIONS=false
TRUST_ROOT_CERTIFICATES=true

# CLI Behavior
MULTIAGENT_AUTO_UPGRADE=false
MULTIAGENT_INTERACTIVE=true
MULTIAGENT_CONFIRM_DESTRUCTIVE=true

# Development Mode
DEVELOPMENT_MODE=false
SKIP_VERSION_CHECK=false
CACHE_CLI_DETECTION=true

# Slack MCP Integration
SLACK_MCP_SERVER_URL=
SLACK_MCP_APP_ID=
SLACK_MCP_DEFAULT_CHANNEL=#security-automation

# Feedback Automation Retention & SLA
APPROVAL_SLA_HOURS=24
TRANSCRIPT_RETENTION_DAYS=90
TRANSCRIPT_STORAGE_PATH=.multiagent/feedback/logs