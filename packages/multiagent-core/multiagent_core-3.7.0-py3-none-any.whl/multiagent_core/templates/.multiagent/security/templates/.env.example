# MultiAgent Project Environment Configuration
# This is a SAFE-TO-COMMIT example file
# Copy to .env and fill in your actual values (NEVER commit .env)

# AI Integration APIs
GEMINI_API_KEY=your-gemini-api-key-here
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
OPENAI_API_KEY=your-openai-api-key-here

# GitHub Integration
GITHUB_TOKEN=your-github-token-here
GITHUB_USERNAME=your-github-username

# Debug and Logging
DEBUG=false
LOG_LEVEL=info

# Project Settings
PROJECT_NAME={{PROJECT_NAME}}
ENVIRONMENT=development

# Security
ALLOW_SUDO_OPERATIONS=false
TRUST_ROOT_CERTIFICATES=true

