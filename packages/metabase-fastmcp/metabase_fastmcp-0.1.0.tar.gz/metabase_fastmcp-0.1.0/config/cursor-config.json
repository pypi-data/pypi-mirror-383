{
  "mcpServers": {
    "metabase-stdio": {
      "command": "uv",
      "args": ["run", "python", "server.py"],
      "cwd": "./python-fastmcp",
      "env": {
        "METABASE_URL": "your-metabase-url-here",
        "METABASE_API_KEY": "your-api-key-here"
      }
    },
    "metabase-sse": {
      "url": "http://localhost:8000/sse",
      "env": {
        "METABASE_URL": "your-metabase-url-here",
        "METABASE_API_KEY": "your-api-key-here"
      }
    }
  }
} 