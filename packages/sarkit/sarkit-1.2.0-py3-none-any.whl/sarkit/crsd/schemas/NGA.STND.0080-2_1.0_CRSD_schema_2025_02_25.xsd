<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
   elementFormDefault="qualified"
   attributeFormDefault="unqualified"
   targetNamespace="http://api.nsgreg.nga.mil/schema/crsd/1.0"
   xmlns="http://api.nsgreg.nga.mil/schema/crsd/1.0"
   xmlns:ns="http://api.nsgreg.nga.mil/schema/crsd/1.0"
   version="2025-02-25">

<!-- CRSD TYPES -->
   
   <!-- - - - - - - - - - - - - - - - - -->
   <!-- Table 11-1 XML Parameter Types  -->
   <!-- - - - - - - - - - - - - - - - - -->

   <xs:simpleType name="XDTType">
      <xs:restriction base="xs:dateTime">
         <xs:pattern value=".*Z"></xs:pattern>
      </xs:restriction>
   </xs:simpleType>

   <xs:complexType name="LSType">
      <xs:sequence>
         <xs:element name="Line" type="xs:double"/>
         <xs:element name="Sample" type="xs:double"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="XYType">
      <xs:sequence>
         <xs:element name="X" type="xs:double"/>
         <xs:element name="Y" type="xs:double"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="XYZType">
      <xs:sequence>
         <xs:element name="X" type="xs:double"/>
         <xs:element name="Y" type="xs:double"/>
         <xs:element name="Z" type="xs:double"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="LatLonType">
      <xs:sequence>
         <xs:element name="Lat" type="xs:double"/>
         <xs:element name="Lon" type="xs:double"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="LatLonRestrictionType">
      <xs:sequence>
         <xs:element name="Lat" type="GE-90_LE90_DoubleType"/>
         <xs:element name="Lon" type="GE-180_LE180_DoubleType"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="LatLonHAERestrictionType">
      <xs:sequence>
         <xs:element name="Lat" type="GE-90_LE90_DoubleType"/>
         <xs:element name="Lon" type="GE-180_LE180_DoubleType"/>
         <xs:element name="HAE" type="xs:double"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="PolyCoef1DType">
      <xs:simpleContent>
         <xs:extension base="xs:double">
            <xs:attribute name="exponent1" type="xs:nonNegativeInteger" use="required"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>

   <xs:complexType name="Poly1DType">
      <xs:sequence>
         <xs:element name="Coef" type="PolyCoef1DType" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="order1" type="xs:nonNegativeInteger" use="required"/>
   </xs:complexType>

   <xs:complexType name="PolyCoef2DType">
      <xs:simpleContent>
         <xs:extension base="xs:double">
            <xs:attribute name="exponent1" type="xs:nonNegativeInteger" use="required"/>
            <xs:attribute name="exponent2" type="xs:nonNegativeInteger" use="required"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>

   <xs:complexType name="Poly2DType">
      <xs:sequence>
         <xs:element name="Coef" type="PolyCoef2DType" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="order1" type="xs:nonNegativeInteger" use="required"/>
      <xs:attribute name="order2" type="xs:nonNegativeInteger" use="required"/>
   </xs:complexType>

   <xs:complexType name="MatrixEntry2DType">
      <xs:simpleContent>
         <xs:extension base="xs:double">
            <xs:attribute name="index1" type="xs:positiveInteger" use="required"/>
            <xs:attribute name="index2" type="xs:positiveInteger" use="required"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>

   <xs:complexType name="Matrix2DType">
      <xs:sequence>
         <xs:element name="Entry" type="MatrixEntry2DType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="ErrorDecorrFuncType">
      <xs:sequence>
         <xs:element name="CorrCoefZero" type="G0_LE1_DoubleType"/>
         <xs:element name="DecorrRate" type="PositiveDoubleType"/>
      </xs:sequence>
   </xs:complexType>

   <!-- - - - - - - - - - - - -->
   <!-- Common derived types  -->
   <!-- - - - - - - - - - - - -->

   <xs:simpleType name="NonEmptyStringType">
      <xs:restriction base="xs:string">
         <xs:minLength value="1"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="E1_IntegerType">
      <xs:restriction base="xs:positiveInteger">
         <xs:enumeration value="1"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="E2_IntegerType">
      <xs:restriction base="xs:positiveInteger">
         <xs:enumeration value="2"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="E3_IntegerType">
      <xs:restriction base="xs:positiveInteger">
         <xs:enumeration value="3"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE2_IntegerType">
      <xs:restriction base="xs:positiveInteger">
         <xs:minInclusive value="2"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE3_IntegerType">
      <xs:restriction base="xs:positiveInteger">
         <xs:minInclusive value="3"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="NonNegativeDoubleType">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="0"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="PositiveDoubleType">
      <xs:restriction base="xs:double">
         <xs:minExclusive value="0"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="G0_LE1_DoubleType">
      <xs:restriction base="xs:double">
         <xs:minExclusive value="0"/>
         <xs:maxInclusive value="1"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE0_LE1_DoubleType">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="0"/>
         <xs:maxInclusive value="1"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE-0.5_LE0.5_DoubleType">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="-0.5"/>
         <xs:maxInclusive value="0.5"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE0_LE90_DoubleType">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="0"/>
         <xs:maxInclusive value="90"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE0_LE180_DoubleType">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="0"/>
         <xs:maxInclusive value="180"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE0_L360_DoubleType">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="0"/>
         <xs:maxExclusive value="360"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE-90_LE90_DoubleType">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="-90"/>
         <xs:maxInclusive value="90"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="GE-180_LE180_DoubleType">
      <xs:restriction base="xs:double">
         <xs:minInclusive value="-180"/>
         <xs:maxInclusive value="180"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="CornerAttrType">
      <xs:restriction base="xs:positiveInteger">
         <xs:minInclusive value="1"/>
         <xs:maxInclusive value="4"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:complexType name="LatLonCornerRestrictionType">
     <xs:complexContent>
      <xs:extension base="LatLonRestrictionType">
         <xs:attribute name="index" type="CornerAttrType" use="required"/>
      </xs:extension>
     </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="LSVertexType">
      <xs:complexContent>
         <xs:extension base="LSType">
            <xs:attribute name="index" type="xs:positiveInteger" use="required"/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Matrix2x2Type">
      <xs:complexContent>
         <xs:extension base="Matrix2DType">
            <xs:attribute name="size1" type="xs:int" fixed="2" use="required"/>
            <xs:attribute name="size2" type="xs:int" fixed="2" use="required"/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Matrix3x3Type">
      <xs:complexContent>
         <xs:extension base="Matrix2DType">
            <xs:attribute name="size1" type="xs:int" fixed="3" use="required"/>
            <xs:attribute name="size2" type="xs:int" fixed="3" use="required"/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Matrix4x4Type">
      <xs:complexContent>
         <xs:extension base="Matrix2DType">
            <xs:attribute name="size1" type="xs:int" fixed="4" use="required"/>
            <xs:attribute name="size2" type="xs:int" fixed="4" use="required"/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Matrix6x6Type">
      <xs:complexContent>
         <xs:extension base="Matrix2DType">
            <xs:attribute name="size1" type="xs:int" fixed="6" use="required"/>
            <xs:attribute name="size2" type="xs:int" fixed="6" use="required"/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="XYPolygonType">
      <xs:sequence>
         <xs:element name="Vertex" minOccurs="3" maxOccurs="unbounded">
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="XYType">
                     <xs:attribute name="index" type="xs:positiveInteger" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="size" type="GE3_IntegerType" use="required"/>
   </xs:complexType>

   <xs:complexType name="AreaType">
      <xs:sequence>
         <xs:element name="X1Y1" type="XYType"/>
         <xs:element name="X2Y2" type="XYType"/>
         <xs:element name="Polygon" type="XYPolygonType"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="ParameterType">
      <xs:simpleContent>
         <xs:extension base="xs:string">
            <xs:attribute name="name" type="NonEmptyStringType" use="required"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>

   <!-- - - - - - - - - - - - - - - - - - -->
   <!-- Table 11-2 Allowed Binary Formats -->
   <!-- - - - - - - - - - - - - - - - - - -->

   <xs:simpleType name="ElementFormatType">
      <xs:restriction base="xs:string">
         <xs:pattern value="([UI][1248]|F[48]|CI(2|4|8|16)|CF(8|16)|S[1-9][0-9]*)|([^=]+=(([UI][1248]|F[48]|CI(2|4|8|16)|CF(8|16)|S[1-9][0-9]*);)){1,}"/>
      </xs:restriction>
   </xs:simpleType>

   <!-- - - - - - - - - - - - - - - -->
   <!-- Table 11-3 XML Enumerations -->
   <!-- - - - - - - - - - - - - - - -->

   <xs:simpleType name="CollectTypeEnum">
      <xs:restriction base="xs:string">
         <xs:enumeration value="MONOSTATIC"/>
         <xs:enumeration value="BISTATIC"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="ModeTypeEnum">
      <xs:restriction base="xs:string">
         <xs:enumeration value="SPOTLIGHT"/>
         <xs:enumeration value="STRIPMAP"/>
         <xs:enumeration value="DYNAMIC STRIPMAP"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="RefHeightEnum">
      <xs:restriction base="xs:string">
         <xs:enumeration value="IARP"/>
         <xs:enumeration value="ZERO"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="EarthModelEnum">
      <xs:restriction base="xs:string">
         <xs:enumeration value="WGS_84"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="SignalArrayFormatEnum">
      <xs:restriction base="ElementFormatType">
         <xs:enumeration value="CI2"/>
         <xs:enumeration value="CI4"/>
         <xs:enumeration value="CF8"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="PolarizationIDEnum">
      <xs:restriction base="xs:string">
         <xs:enumeration value="X"/>
         <xs:enumeration value="Y"/>
         <xs:enumeration value="V"/>
         <xs:enumeration value="H"/>
         <xs:enumeration value="S"/>
         <xs:enumeration value="E"/>
         <xs:enumeration value="RHC"/>
         <xs:enumeration value="LHC"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="TxWaveformEnum">
      <xs:restriction base="xs:string">
         <xs:enumeration value="LFM"/>
         <xs:enumeration value="LFM w XM"/>
         <xs:enumeration value="XM"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="SideOfTrackEnum">
      <xs:restriction base="xs:string">
         <xs:enumeration value="L"/>
         <xs:enumeration value="R"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="FrameEnum">
      <xs:restriction base="xs:string">
         <xs:enumeration value="ECF"/>
         <xs:enumeration value="RIC_ECF"/>
         <xs:enumeration value="RIC_ECI"/>
      </xs:restriction>
   </xs:simpleType>

<!-- CRSD BLOCKS -->

   <!-- - - - - - - - - - - - - - - - - - -->
   <!-- Table 12-1(a) Product Information -->
   <!-- - - - - - - - - - - - - - - - - - -->

   <xs:complexType name="ProductInformationType">
      <xs:sequence>
         <xs:element name="ProductName" type="NonEmptyStringType"/>
         <xs:element name="Classification" type="xs:string"/>
         <xs:element name="ReleaseInfo" type="xs:string"/>
         <xs:element name="CountryCode" type="xs:string" minOccurs="0"/>
         <xs:element name="Profile" type="NonEmptyStringType" minOccurs="0"/>
         <xs:element name="CreationInfo" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="Application" type="NonEmptyStringType"/>
                  <xs:element name="DateTime" type="XDTType"/>
                  <xs:element name="Site" type="NonEmptyStringType" minOccurs="0"/>
                  <xs:element name="Parameter" type="ParameterType" minOccurs="0" maxOccurs="unbounded"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="Parameter" type="ParameterType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>

   <!-- - - - - - - - - - - - - - - - - - - - -->
   <!-- Table 12-1(b) SAR Imaging Information -->
   <!-- - - - - - - - - - - - - - - - - - - - -->

   <xs:complexType name="SARImagingInformationType">
      <xs:sequence>
         <xs:element name="CollectType" type="CollectTypeEnum"/>
         <xs:element name="RadarMode">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="ModeType" type="ModeTypeEnum"/>
                  <xs:element name="ModeID" type="NonEmptyStringType" minOccurs="0"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="Parameter" type="ParameterType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>

   <!-- - - - - - - - - - - - - - - - - - - - - - -->
   <!-- Table 12-1(c) Transmit Sensor Information -->
   <!-- Table 12-1(d) Receive Sensor Information  -->
   <!-- - - - - - - - - - - - - - - - - - - - - - -->
   <xs:complexType name="SensorInformationType">
      <xs:sequence>
         <xs:element name="SensorName" type="NonEmptyStringType"/>
         <xs:element name="EventName" type="NonEmptyStringType"/>
         <xs:element name="Parameter" type="ParameterType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>

   <!-- - - - - - - - - - - - - - - - -->
   <!-- Table 12-2 Global Parameters  -->
   <!-- - - - - - - - - - - - - - - - -->

   <xs:complexType name="GlobalParametersBaseType">
      <xs:sequence>
         <xs:element name="CollectionRefTime" type="XDTType"/>
         <xs:element name="TropoParameters" minOccurs="0">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="N0" type="NonNegativeDoubleType"/>
                  <xs:element name="RefHeight" type="RefHeightEnum"/>
                  <xs:element name="N0ErrorStdDev" type="NonNegativeDoubleType" minOccurs="0"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="IonoParameters" minOccurs="0">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="TECV" type="NonNegativeDoubleType"/>
                  <xs:element name="F2Height" type="NonNegativeDoubleType"/>
                  <xs:element name="TECVErrorStdDev" type="NonNegativeDoubleType" minOccurs="0"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="GlobalTransmitType">
      <xs:sequence>
         <xs:element name="TxTime1" type="xs:double"/>
         <xs:element name="TxTime2" type="xs:double"/>
         <xs:element name="FxMin" type="NonNegativeDoubleType"/>
         <xs:element name="FxMax" type="NonNegativeDoubleType"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="GlobalReceiveType">
      <xs:sequence>
         <xs:element name="RcvStartTime1" type="xs:double"/>
         <xs:element name="RcvStartTime2" type="xs:double"/>
         <xs:element name="FrcvMin" type="NonNegativeDoubleType"/>
         <xs:element name="FrcvMax" type="NonNegativeDoubleType"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="GlobalParametersSARType">
      <xs:complexContent>
         <xs:extension base="GlobalParametersBaseType">
            <xs:sequence>
               <xs:element name="Transmit" type="GlobalTransmitType"/>
               <xs:element name="Receive" type="GlobalReceiveType"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="GlobalParametersTxType">
      <xs:complexContent>
         <xs:extension base="GlobalParametersBaseType">
            <xs:sequence>
               <xs:element name="Transmit" type="GlobalTransmitType"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="GlobalParametersRcvType">
      <xs:complexContent>
         <xs:extension base="GlobalParametersBaseType">
            <xs:sequence>
               <xs:element name="Receive" type="GlobalReceiveType"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <!-- - - - - - - - - - - - - - - - - - - - - -->
   <!-- Table 12-3 Scene Geographic Coordinates -->
   <!-- - - - - - - - - - - - - - - - - - - - - -->

   <xs:complexType name="SceneCoordinatesBaseType">
      <xs:sequence>
         <xs:element name="EarthModel" type="EarthModelEnum"/>
         <xs:element name="IARP">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="ECF" type="XYZType"/>
                  <xs:element name="LLH" type="LatLonHAERestrictionType"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="ReferenceSurface">
            <xs:complexType>
               <xs:sequence>
                  <xs:choice>
                     <xs:element name="Planar">
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="uIAX" type="XYZType"/>
                              <xs:element name="uIAY" type="XYZType"/>
                           </xs:sequence>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="HAE">
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="uIAXLL" type="LatLonType"/>
                              <xs:element name="uIAYLL" type="LatLonType"/>
                           </xs:sequence>
                        </xs:complexType>
                     </xs:element>
                  </xs:choice>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="ImageArea" type="AreaType"/>
         <xs:element name="ImageAreaCornerPoints">
            <xs:complexType>
               <xs:sequence>
                     <xs:element name="IACP" type="LatLonCornerRestrictionType" minOccurs="4" maxOccurs="4"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="SceneCoordinatesSARType">
      <xs:complexContent>
         <xs:extension base="SceneCoordinatesBaseType">
            <xs:sequence>
               <xs:element name="ExtendedArea" type="AreaType" minOccurs="0"/>
               <xs:element name="ImageGrid" minOccurs="0">
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element name="Identifier" type="NonEmptyStringType" minOccurs="0"/>
                        <xs:element name="IARPLocation" type="LSType"/>
                        <xs:element name="IAXExtent">
                           <xs:complexType>
                              <xs:sequence>
                                 <xs:element name="LineSpacing" type="PositiveDoubleType"/>
                                 <xs:element name="FirstLine" type="xs:integer"/>
                                 <xs:element name="NumLines" type="xs:positiveInteger"/>
                              </xs:sequence>
                           </xs:complexType>
                        </xs:element>
                        <xs:element name="IAYExtent">
                           <xs:complexType>
                              <xs:sequence>
                                 <xs:element name="SampleSpacing" type="PositiveDoubleType"/>
                                 <xs:element name="FirstSample" type="xs:integer"/>
                                 <xs:element name="NumSamples" type="xs:positiveInteger"/>
                              </xs:sequence>
                           </xs:complexType>
                        </xs:element>
                        <xs:element name="SegmentList" minOccurs="0">
                           <xs:complexType>
                              <xs:sequence>
                                 <xs:element name="NumSegments" type="xs:positiveInteger"/>
                                 <xs:element name="Segment" maxOccurs="unbounded">
                                    <xs:complexType>
                                       <xs:sequence>
                                          <xs:element name="Identifier" type="NonEmptyStringType"/>
                                          <xs:element name="StartLine" type="xs:integer"/>
                                          <xs:element name="StartSample" type="xs:integer"/>
                                          <xs:element name="EndLine" type="xs:integer"/>
                                          <xs:element name="EndSample" type="xs:integer"/>
                                          <xs:element name="SegmentPolygon">
                                             <xs:complexType>
                                                <xs:sequence>
                                                   <xs:element name="SV" type="LSVertexType" minOccurs="3" maxOccurs="unbounded"/>
                                                </xs:sequence>
                                                <xs:attribute name="size" type="GE3_IntegerType" use="required"/>
                                             </xs:complexType>
                                          </xs:element>
                                       </xs:sequence>
                                    </xs:complexType>
                                 </xs:element>
                              </xs:sequence>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <!-- - - - - - - - - - - - - - - - - - -->
   <!-- Table 12-4 Binary Data Parameters -->
   <!-- - - - - - - - - - - - - - - - - - -->

   <xs:complexType name="SupportArraySizeType">
      <xs:sequence>
         <xs:element name="SAId" type="NonEmptyStringType"/>
         <xs:element name="NumRows" type="xs:positiveInteger"/>
         <xs:element name="NumCols" type="xs:positiveInteger"/>
         <xs:element name="BytesPerElement" type="xs:positiveInteger"/>
         <xs:element name="ArrayByteOffset" type="xs:nonNegativeInteger"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="BinaryDataTransmitType">
      <xs:sequence>
         <xs:element name="NumBytesPPP" type="xs:positiveInteger"/>
         <xs:element name="NumTxSequences" type="xs:positiveInteger"/>
         <xs:element name="TxSequence" maxOccurs="unbounded">
