# ===========================================
# 应用程序配置
# ===========================================
AEROSPOT_DEBUG=false
AEROSPOT_LOG_LEVEL=INFO
AEROSPOT_MAX_WORKERS=4
AEROSPOT_TIMEOUT=30
AEROSPOT_MAX_RETRIES=3
AEROSPOT_CACHE_ENABLED=true
AEROSPOT_CACHE_DIR=cache

# ===========================================
# 日志配置
# ===========================================
AEROSPOT_LOG_FILE_ENABLED=true
AEROSPOT_LOG_FILE_PATH=logs/aerospotreport.log

# ===========================================
# 高级配置
# ===========================================
# 处理配置
AEROSPOT_CHUNK_SIZE=8192

# 缓存配置
AEROSPOT_CACHE_TTL_DAYS=3

# 性能配置
AEROSPOT_ENABLE_PERFORMANCE_MONITORING=true
AEROSPOT_ENABLE_PARALLEL_PROCESSING=true

# ===========================================
# 开发者配置
# ===========================================
# 启用详细的调试信息
DEBUG_VERBOSE=false

# 启用性能分析
DEBUG_PROFILING=false

# 跳过某些验证（仅用于开发）
DEBUG_SKIP_VALIDATION=false