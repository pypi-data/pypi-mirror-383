graft docs
graft pysiglib
graft siglib/cpsig
graft siglib/cusig

include avx_info/avx_info.cpp
include build_cusig.bat
include build_cusig.sh
include build_utils.py
include LICENSE
include README.md
include requirements.txt
include setup.py

prune docs/_build
prune docs/_templates
prune siglib/cpsig/bin
prune siglib/cpsig/x64
prune siglib/cusig/bin
prune siglib/cusig/x64

recursive-exclude pysiglib *.dll
recursive-exclude pysiglib *.dylib
recursive-exclude pysiglib *.so
recursive-exclude siglib *.vcxproj
recursive-exclude siglib *.vcxproj.filters
recursive-exclude siglib *.vcxproj.user
recursive-exclude siglib *.jam
recursive-exclude docs *.psd

prune */__pycache__
prune pysiglib/torch_api/__pycache__

prune pysiglib/.idea
exclude pysiglib/_build_log.txt
exclude pysiglib/_config.py
