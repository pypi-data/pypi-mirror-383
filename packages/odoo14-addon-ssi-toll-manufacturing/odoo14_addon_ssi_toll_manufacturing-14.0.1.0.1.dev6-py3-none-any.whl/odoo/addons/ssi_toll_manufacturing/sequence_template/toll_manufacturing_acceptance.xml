<odoo>
    <record
        id="sequence_template_toll_manufacturing_acceptance"
        model="sequence.template"
    >
        <field name="name">Standard</field>
        <field name="model_id" ref="model_toll_manufacturing_acceptance" />
        <field name="sequence" eval="100" />
        <field name="initial_string">/</field>
        <field
            name="sequence_field_id"
            search="[('model_id.model','=','toll_manufacturing_acceptance'),('name','=','name')]"
        />
        <field
            name="date_field_id"
            search="[('model_id.model','=','toll_manufacturing_acceptance'),('name','=','date')]"
        />
        <field name="computation_method">use_python</field>
        <field name="python_code">result=True</field>
        <field
            name="sequence_id"
            ref="ssi_toll_manufacturing.sequence_toll_manufacturing_acceptance"
        />
        <field name="sequence_selection_method">use_sequence</field>
        <field name="add_custom_prefix" eval="0" />
        <field name="add_custom_suffix" eval="0" />
    </record>
</odoo>
