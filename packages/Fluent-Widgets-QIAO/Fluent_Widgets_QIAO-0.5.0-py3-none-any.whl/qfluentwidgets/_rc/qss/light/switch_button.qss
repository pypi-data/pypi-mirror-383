QWidget {
    background-color: white;
}

SwitchButton {
    qproperty-spacing: 12;
    background-color: transparent;
}

SwitchButton>QLabel {
    color: black;
    font-family: --FontFamily;
    font-size: 14px;
    background-color: transparent;
    border: none;
}

SwitchButton>QLabel:disabled {
    color: rgba(0, 0, 0, 0.36);
}

