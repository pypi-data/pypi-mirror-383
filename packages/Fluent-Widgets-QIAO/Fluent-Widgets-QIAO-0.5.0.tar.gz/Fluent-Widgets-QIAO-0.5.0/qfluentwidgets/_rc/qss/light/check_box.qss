CheckBox {
    color: black;
    font-family: --FontFamily;
    font-size: 14px;
    spacing: 8px;
    min-width: 28px;
    min-height: 22px;
    outline: none;
    margin-left: 1px;
}

CheckBox::indicator {
    width: 18px;
    height: 18px;
    border-radius: 5px;
    border: 1px solid transparent;
    background-color: transparent;
}

CheckBox:disabled {
    color: rgba(0, 0, 0, 0.36);
}
