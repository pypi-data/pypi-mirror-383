 <transition transition_id="accept"
             title="accept"
             new_state="accepted"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" icon="%(portal_url)s/accept.png" category="workflow" i18n:translate="">accept</action>
  <guard>
    <guard-permission>Review portal content</guard-permission>
  </guard>
 </transition>
