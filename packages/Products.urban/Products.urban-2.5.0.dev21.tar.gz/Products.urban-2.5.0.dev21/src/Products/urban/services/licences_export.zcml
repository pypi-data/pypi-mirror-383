<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser"
           xmlns:i18n="http://namespaces.zope.org/i18n"
           i18n_domain="urban">

  <browser:view
     for="Products.CMFPlone.interfaces.siteroot.IPloneSiteRoot"
     name="get_licence_types"
     class=".licences_export.get_licence_types"
     permission="zope2.View"
  />

  <browser:view
     for="Products.CMFPlone.interfaces.siteroot.IPloneSiteRoot"
     name="get_licence_states"
     class=".licences_export.get_licence_states"
     permission="zope2.View"
  />

  <browser:view
     for="Products.CMFPlone.interfaces.siteroot.IPloneSiteRoot"
     name="export_licences"
     class=".licences_export.export_licences"
     permission="zope2.View"
  />

</configure>
