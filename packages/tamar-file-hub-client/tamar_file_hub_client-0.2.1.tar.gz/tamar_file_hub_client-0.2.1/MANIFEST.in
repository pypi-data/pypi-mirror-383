# 包含README和许可证文件
include README.md
include LICENSE

# 包含proto文件
recursive-include file_hub_client/rpc/protos *.proto

# 包含类型标记文件
include file_hub_client/py.typed

# 排除测试和示例
prune tests
prune examples
prune docs

# 排除开发文件
exclude .gitignore
exclude .pre-commit-config.yaml
exclude tox.ini
exclude Makefile
exclude .coverage
exclude .pytest_cache

# 排除编译文件
global-exclude *.py[cod]
global-exclude __pycache__
global-exclude *.so
global-exclude *.dylib 