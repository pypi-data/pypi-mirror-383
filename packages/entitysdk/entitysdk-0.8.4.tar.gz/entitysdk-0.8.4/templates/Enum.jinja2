{% for decorator in decorators -%}
{{ decorator }}
{% endfor -%}
{% if base_class == 'str, Enum' %}
class {{ class_name }}(StrEnum):
{% else %}
class {{ class_name }}({{ base_class }}):
{% endif %}
{%- if description %}
    """
    {{ description | indent(4) }}
    """
{%- endif %}
{%- for field in fields %}
    {{ field.name }} = {{ field.default }}
    {%- if field.docstring %}
    """
    {{ field.docstring | indent(4) }}
    """
    {%- endif %}
{%- endfor -%}



