```python
from kaggler.core.mutation import MutationStrategy, MutationType

class DomainSpecificMutation(MutationStrategy):
    """ãƒ‰ãƒ¡ã‚¤ãƒ³å›ºæœ‰ã®å¤‰ç•°æˆ¦ç•¥"""

    def mutate(self, prompt: str, context=None):
        # ãƒ‰ãƒ¡ã‚¤ãƒ³çŸ¥è­˜ã‚’ä½¿ã£ãŸå¤‰ç•°
        if "é‡‘è" in prompt:
            return self.add_financial_rules(prompt)
        elif "åŒ»ç™‚" in prompt:
            return self.add_medical_constraints(prompt)
        else:
            return super().mutate(prompt, context)

    def add_financial_rules(self, prompt: str):
        """é‡‘èå›ºæœ‰ã®ãƒ«ãƒ¼ãƒ«ã‚’è¿½åŠ """
        rules = """

## é‡‘èãƒ‡ãƒ¼ã‚¿å‡¦ç†ãƒ«ãƒ¼ãƒ«
- é€šè²¨è¨˜å·ã‚’å‰Šé™¤ï¼ˆ$, Â¥, â‚¬ãªã©ï¼‰
- ãƒ‘ãƒ¼ã‚»ãƒ³ãƒ†ãƒ¼ã‚¸ã¯å°æ•°ã§è¡¨ç¾ï¼ˆ5% â†’ 0.05ï¼‰
- æ—¥ä»˜ã¯ISO 8601å½¢å¼ï¼ˆYYYY-MM-DDï¼‰
        """
        return prompt + rules

    def add_medical_constraints(self, prompt: str):
        """åŒ»ç™‚å›ºæœ‰ã®åˆ¶ç´„ã‚’è¿½åŠ """
        constraints = """

## åŒ»ç™‚ãƒ‡ãƒ¼ã‚¿æ¤œè¨¼
- ãƒã‚¤ã‚¿ãƒ«ã‚µã‚¤ãƒ³ç¯„å›²ãƒã‚§ãƒƒã‚¯
- è–¬å‰¤ç›¸äº’ä½œç”¨ã®è­¦å‘Š
- å˜ä½ã®æ¨™æº–åŒ–ï¼ˆmg, mL, etcï¼‰
        """
        return prompt + constraints

# ä½¿ç”¨ä¾‹
custom_strategy = DomainSpecificMutation()
evolved_prompt = custom_strategy.mutate("æ‚£è€…ãƒ‡ãƒ¼ã‚¿ã‚’æŠ½å‡º")
```

