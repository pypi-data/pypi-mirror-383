{
  "type": "html",
  "url": "https://www.technewsworld.com/archive/page/{}",
  "pagination": true,
  "pagination_limit": 2,
  "root": {
    "selector": "div.category-article-list",
    "attribute": "element"
  },
  "entry": {
    "selector": "div.search-item",
    "attribute": "element",
    "item_type": "list"
  },
  "elements": {
    "title": {
      "selector": "div.search-txt a h2",
      "attribute": "text"
    },
    "url": {
      "selector": "div.search-txt a",
      "attribute": "href"
    }
  }
}