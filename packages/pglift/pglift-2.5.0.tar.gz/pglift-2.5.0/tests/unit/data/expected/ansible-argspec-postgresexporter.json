{
  "dsn": {
    "description": [
      "connection string of target instance"
    ],
    "required": true,
    "type": "str"
  },
  "name": {
    "description": [
      "locally unique identifier of the service"
    ],
    "required": true,
    "type": "str"
  },
  "password": {
    "description": [
      "connection password"
    ],
    "no_log": true,
    "type": "str"
  },
  "port": {
    "description": [
      "TCP port for the web interface and telemetry"
    ],
    "required": true,
    "type": "int"
  },
  "state": {
    "choices": [
      "started",
      "stopped",
      "absent"
    ],
    "default": "started",
    "description": [
      "runtime state"
    ]
  }
}
