apiVersion: v1
kind: Tool
metadata:
  name: {{ tool_name }}
  label:
    en_US: {{ tool_label }}
    zh_Hans: {{ tool_label }}
  description:
    en_US: '{{ tool_description }}'
    zh_Hans: '{{ tool_description }}'
spec:
  parameters: {}
  llm_prompt: '{{ tool_description }}'
execution:
  python:
    path: {{ tool_name }}.py
    attr: {{ tool_attr }}
