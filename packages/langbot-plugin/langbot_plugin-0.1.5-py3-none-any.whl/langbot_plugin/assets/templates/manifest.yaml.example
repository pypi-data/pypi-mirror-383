apiVersion: v1
kind: Plugin
metadata:
  author: {{ plugin_author }}
  name: {{ plugin_name }}
  repository: '{{ plugin_repository }}'
  version: 0.1.0
  description:
    en_US: '{{ plugin_description }}'
    zh_Hans: '{{ plugin_description }}'
  label:
    en_US: {{ plugin_label }}
    zh_Hans: {{ plugin_label }}
  icon: assets/icon.svg  # replace in assets/icon.svg
spec:
  config: []
  components: {}
execution:
  python:
    path: main.py
    attr: {{ plugin_attr }}