    "requirements": [
        "aws-cli>=2.23.2",
        "bluecellulab>=2.6.40",
        "neuron>=8.0<9.0",
        "matplotlib>=3.7.1",
        "bluepysnap>=3.0.1",
        "h5py>=3.13.0" 
    ]
}