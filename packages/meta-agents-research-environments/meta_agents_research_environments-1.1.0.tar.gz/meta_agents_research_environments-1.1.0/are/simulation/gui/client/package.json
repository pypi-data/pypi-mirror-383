{
  "name": "are-simulation-client-vite-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tsc": "tsc -b",
    "build-no-tsc": "vite build",
    "build": "tsc -b && vite build",
    "test": "vitest",
    "relay": "relay-compiler",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "graphql": "cd ../../..; strawberry export-schema are-simulation.gui.server.graphql.schema:schema --output are-simulation/gui/client/schema.graphql; cd -",
    "update": "npm i && npm run graphql && npm run relay",
    "clean": "git clean -fdx --exclude='.env'",
    "format": "npx prettier --write ./src"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.5",
    "@emotion/is-prop-valid": "^1.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/x-data-grid": "^8.7.0",
    "@mui/x-date-pickers": "^8.7.0",
    "@mui/x-tree-view": "^8.7.0",
    "@types/uuid": "^10.0.0",
    "@xyflow/react": "^12.8.1",
    "dayjs": "^1.11.10",
    "dotenv": "^17.0.1",
    "dotenv-expand": "^12.0.2",
    "framer-motion": "^12.23.0",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.5",
    "html-to-image": "^1.11.13",
    "motion": "^12.23.0",
    "nanoid": "^5.1.5",
    "print-js": "^1.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-relay": "^20.1.1",
    "react-resizable": "^3.0.5",
    "relay-runtime": "^20.0.0",
    "uuid": "^13.0.0",
    "vite-plugin-relay": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-relay": "^18.2.1",
    "@types/react-resizable": "^3.0.8",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "relay-compiler": "^20.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.2",
    "vitest": "^3.2.4"
  },
  "relay": {
    "src": "./src",
    "schema": "../client/schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  }
