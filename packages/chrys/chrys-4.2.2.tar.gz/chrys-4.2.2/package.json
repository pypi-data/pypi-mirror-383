{
  "type": "module",
  "name": "chrys",
  "version": "4.2.2",
  "description": "A collection of color palettes for visualisation in JavaScript, Python and Sass",
  "main": "cjs/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/netbek/chrys"
  },
