{
  "name": "module-name",
  "version": "1.0.0",
  "type": "with-ui",
  "computeType": "docker",
  "description": "Module with user interface",
  "entrypoint": "backend.main:UIModule",
  "author": {
    "name": "Developer Name",
    "email": "developer@example.com",
    "organization": "Organization"
  },
  "runtime": "python3.11",
  "ui": {
    "framework": "react",
    "buildCommand": "npm run build",
    "distPath": "frontend/dist"
  },
  "inputs": {
    "param1": {
      "type": "string",
      "description": "Primary input parameter",
      "required": true
    },
    "param2": {
      "type": "string",
      "description": "Optional parameter",
      "required": false,
      "default": "default_value"
    }
  },
  "outputs": {
    "results": {
      "type": "array",
      "description": "Processed results for UI display"
    },
    "summary": {
      "type": "object",
      "description": "Summary statistics"
    }
  },
  "resources": {
    "memory": 512,
    "timeout": 300,
    "environment": {
      "LOG_LEVEL": "INFO"
    }
  },
  "permissions": {
    "database": ["read"],
    "storage": true
  },
  "dependencies": {
    "peptides": true,
    "storage": true,
    "api": true
  }
}
