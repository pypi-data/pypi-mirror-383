{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "properties",
  "rules": {
    "file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_space"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "property"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "comment"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_eol"
              },
              {
                "type": "SYMBOL",
                "name": "_eof"
              }
            ]
          }
        ]
      }
    },
    "property": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "key"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_space"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "="
                  },
                  {
                    "type": "STRING",
                    "value": ":"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_space"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "key": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "SYMBOL",
              "name": "_char"
            },
            {
              "type": "SYMBOL",
              "name": "escape"
            },
            {
              "type": "SYMBOL",
              "name": "_index"
            }
          ]
        }
      }
    },
    "_index": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_char"
            }
          },
          "named": true,
          "value": "index"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "value": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_char"
          },
          {
            "type": "SYMBOL",
            "name": "escape"
          },
          {
            "type": "SYMBOL",
            "name": "substitution"
          },
          {
            "type": "SYMBOL",
            "name": "_linebreak"
          }
        ]
      }
    },
    "substitution": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "key"
              },
              {
                "type": "SYMBOL",
                "name": "substitution"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_default"
                },
                {
                  "type": "SYMBOL",
                  "name": "_secret"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_default": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "default",
              "content": {
                "type": "SYMBOL",
                "name": "_content"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_secret": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "::"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_content"
          },
          "named": true,
          "value": "secret"
        }
      ]
    },
    "_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_char"
          },
          {
            "type": "SYMBOL",
            "name": "escape"
          },
          {
            "type": "SYMBOL",
            "name": "substitution"
          }
        ]
      }
    },
    "_linebreak": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "escape": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\\\[^\\r\\n]"
        },
        {
          "type": "PATTERN",
          "value": "\\\\u[0-9a-fA-F]{4}"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "[#!].*"
    },
    "_space": {
      "type": "PATTERN",
      "value": "[ \\t\\f]"
    },
    "_char": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "PATTERN",
        "value": "."
      }
    },
    "_eol": {
      "type": "PATTERN",
      "value": "[\\r\\n]|\\r\\n"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_eof"
    }
  ],
  "inline": [],
  "supertypes": []
}
